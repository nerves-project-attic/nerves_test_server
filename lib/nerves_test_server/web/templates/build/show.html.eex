<h2>Build Status</h2>

<table class="table table-sm">
  <thead>
    <tr>
      <th>Status</th>
      <th>Checks</th>
      <th>Failed</th>
      <th>Skipped</th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <td><%= @build.status %></td>
        <%= for metric <- ["total", "failures", "skipped"] do %>
          <td><%= @build.result[metric] %></td>
        <% end %>
      </tr>
  </tbody>
</table>
<h2>Build Output</h2>

<pre><code>
  <%= @build.result_io %>
</code></pre>
